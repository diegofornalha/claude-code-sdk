# Configuração do Coverage.py

[run]
source = core, server
omit =
    */tests/*
    */test_*.py
    */__pycache__/*
    */venv/*
    */env/*
    */site-packages/*
    */migrations/*
    */sdk/tests/*
    */scripts/*
    */examples/*
    */credentials/*
    */mcp-proxy/*

[report]
precision = 2
show_missing = True
skip_covered = False
exclude_lines =
    pragma: no cover
    def __repr__
    raise AssertionError
    raise NotImplementedError
    if __name__ == .__main__.:
    if TYPE_CHECKING:
    @abstractmethod
    @abc.abstractmethod
    class .*\bProtocol\):
    @overload

[html]
directory = htmlcov
title = Neo4j Agent API - Coverage Report